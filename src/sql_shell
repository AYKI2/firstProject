postgres=# CREATE TABLE car(id SERIAL PRIMARY KEY,model VARCHAR(25),price INTEGER);
CREATE TABLE
postgres=# INSERT INTO car(model,price)VALUES ('M5',123),('911 GT3',456),('Maybach',789),('Phantom',123456);
