postgres=# CREATE TABLE cart(id SERIAL PRIMARY KEY, user_id INTEGER, cart_number VARCHAR(50), FOREIGN KEY(user_id) REFERENCES users(id));
CREATE TABLE
postgres=# SELECT * FROM cart;
 id | user_id | cart_number 
----+---------+-------------
(0 rows)

